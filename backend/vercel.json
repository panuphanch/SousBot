{
  "version": 2,
  "public": true,
  "builds": [
    {
      "src": "src/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/webhook",
      "methods": ["POST"],
      "dest": "/src/index.ts"
    },
    {
      "src": "/health",
      "methods": ["GET"],
      "dest": "/src/index.ts"
    },
    {
      "src": "/api/debug-log",
      "methods": ["POST"],
      "dest": "/src/index.ts"
    },
    {
      "src": "/api/products/([^/]+)",
      "methods": ["GET"],
      "dest": "/src/index.ts"
    },
    {
      "src": "/api/products/([^/]+)/availability",
      "methods": ["PATCH"],
      "dest": "/src/index.ts"
    },
    {
      "src": "/register",
      "methods": ["POST"],
      "dest": "/src/index.ts"
    }
  ]
}